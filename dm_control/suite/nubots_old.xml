        <motor name='j_left_shoulder_pitch' joint='j_left_shoulder_pitch' gear="20"/>
        <motor name='j_left_shoulder_roll' joint='j_left_shoulder_roll' gear="20"/>
        <motor name='j_left_elbow_pitch' joint='j_left_elbow_pitch' gear="20"/>
        <motor name='j_right_shoulder_pitch' joint='j_right_shoulder_pitch' gear="20"/>
        <motor name='j_right_shoulder_roll' joint='j_right_shoulder_roll' gear="20"/>
        <motor name='j_right_elbow_pitch' joint='j_right_elbow_pitch' gear="20"/>
    </actuator>

    <sensor>
      <subtreelinvel name="torso_subtreelinvel" body="torso"/>
      <accelerometer name="torso_accel"    site="torso"/>
      <velocimeter name="torso_vel"        site="torso"/>
      <gyro name="torso_gyro"              site="torso"/>

      <velocimeter name="sensor_root_veloc" site="torso"/>
      <gyro name="sensor_root_gyro" site="torso"/>
      <accelerometer name="sensor_root_accel" site="torso"/>
      <touch name="sensor_touch_ltoes" site="left_foot_touch"/>
      <touch name="sensor_touch_rtoes" site="right_foot_touch"/>
    </sensor>
</mujoco>



